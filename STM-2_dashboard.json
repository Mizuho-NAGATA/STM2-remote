{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "panels": [
    {
      "datasource": { "type": "influxdb", "uid": "ffa7izoai6m80b" },
      "id": 1,
      "title": "Thickness 80% Alert",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "query": "SELECT last(\"alert_state\") FROM \"stm2_settings\" WHERE $timeFilter AND \"run_id\" =~ /^$run_id$/ GROUP BY \"run_id\"",
          "rawQuery": true,
          "refId": "A",
          "resultFormat": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": {
                  "text": "目標厚さの80％を超えました",
                  "color": "dark-red"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      }
    },

    {
      "datasource": { "type": "influxdb", "uid": "ffa7izoai6m80b" },
      "id": 2,
      "title": "Thickness [nm]",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "query": "SELECT last(\"thickness\") FROM \"stm2\" WHERE $timeFilter AND \"run_id\" =~ /^$run_id$/ GROUP BY \"run_id\"",
          "rawQuery": true,
          "refId": "A"
        }
      ]
    },

    {
      "datasource": { "type": "influxdb", "uid": "ffa7izoai6m80b" },
      "id": 3,
      "title": "Target Thickness [nm]",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "query": "SELECT last(\"target_thickness\") FROM \"stm2_settings\" WHERE $timeFilter AND \"run_id\" =~ /^$run_id$/ GROUP BY \"run_id\"",
          "rawQuery": true,
          "refId": "A"
        }
      ]
    },

    {
      "datasource": { "type": "influxdb", "uid": "ffa7izoai6m80b" },
      "id": 4,
      "title": "Frequency [Hz]",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "query": SELECT last("frequency")
          FROM "stm2"
          WHERE $timeFilter AND "run_id" =~ /^$run_id$/
          GROUP BY "run_id"",
          "rawQuery": true,
          "refId": "A"
        }
      ]
    },

    {
      "datasource": { "type": "influxdb", "uid": "ffa7izoai6m80b" },
      "id": 5,
      "title": "Thickness Table",
      "type": "table",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 },
      "targets": [
        {
          "query": "SELECT last(\"thickness\") FROM \"stm2\" WHERE $timeFilter AND \"run_id\" =~ /^$run_id$/ GROUP BY \"run_id\", \"material\",\"density\",\"z_ratio\"",
          "rawQuery": true,
          "resultFormat": "table",
          "refId": "A"
        }
      ]
    }
  ],

  "templating": {
    "list": [
      {
        "name": "run_id",
        "type": "query",
        "datasource": { "type": "influxdb", "uid": "ffa7izoai6m80b" },
        "query": "SHOW TAG VALUES FROM \"stm2\" WITH KEY = \"run_id\"",
        "refresh": 1,
        "includeAll": false,
        "multi": false
      }
    ]
  },

  "time": { "from": "now-5m", "to": "now" },
  "timezone": "browser",
  "title": "STM-2",
  "schemaVersion": 42,
  "version": 1
}
